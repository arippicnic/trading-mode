{
  "name": "0-tradingmode",
  "version": "1.1.0",
  "description": "Tradingview price tracker IDR",
  "main": "./buildServer/index.js",
  "types": "./buildServer/index.d.ts",
  "files": [
    "buildServer/**/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run clean\" \"sleep 5 && nodemon\"",
    "build": "concurrently \"npm run clean\" \"sleep 5 && next build && tsc --project tsconfig.server.json\"",
    "start": "node ./buildServer/index.js",
    "lint": "next lint",
    "copy": "copyfiles --flat src/pages/api/_cache/_data/info.json src/pages/api/_cache",
    "clean": "rimraf node_modules/.cache .next src/pages/api/_cache/info.json buildServer && npm run copy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.4",
    "classnames": "^2.3.1",
    "concurrently": "^7.1.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "mongoose": "^6.3.1",
    "next": "^12.1.5",
    "next-connect": "^0.12.2",
    "next-themes": "^0.1.1",
    "nextjs-progressbar": "^0.0.14",
    "node-cron": "^3.0.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "react-tooltip": "^4.2.21",
    "sass": "^1.50.1",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "@types/node-cron": "^3.0.1",
    "@types/react": "^18.0.6",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
