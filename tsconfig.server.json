      "sourceMap": true,
      "target": "es5",
      "module": "commonjs",
      "declaration": true,
      "outDir": "./buildServer",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
    },
    "include": ["server/**/*"],
